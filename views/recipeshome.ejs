<!DOCTYPE html>

<html lang="en">

<head>
<meta charset="UTF-8">
<!-- Bootstrap CSS -->
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC"
      crossorigin="anonymous"
    />
<title>Recipe list</title>
</head>

<body>
  <div class="container-fluid text-center w-75">
      <nav class="navbar navbar-light bg-light justify-content-center">
        <a class="navbar-brand" href="/">Home</a>
      </nav>
<h1 id="header"><% recipehome.forEach((item) => { %>
        <h1 id="header"><%= item.title %></h1>
      <% });
    %></h1>
<% recipehome.forEach((item) => { %>
        <img src = "<%= item.image %>" alt="<%= item.imagealt %>" width="40%" height="40%"></img>
      <% });
    %>

<h2 class="mt-5">Staff picks</h2>

<div id="recipes-list">
  <ul>
    <% recipehome.forEach((item) => { %>
        <a href="<%= item.ahref1 %>">
          <% recipehome.forEach((value) => { %>
        <li><%= value.list[0] %></h1>
      <% });
    %>
        </a>
      <% });
    %> 
    <% recipehome.forEach((item) => { %>
        <a href="<%= item.ahref2 %>">
          <% recipehome.forEach((value) => { %>
        <li><%= value.list[1] %></h1>
      <% });
    %>
        </a>
      <% });
    %> 
  </ul>
</div>
</div>
</body>

</html>